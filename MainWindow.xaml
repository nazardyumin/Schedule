                                  ItemsSource="{Binding AddingSection.DatesTo, Mode=TwoWay}" 
                                  SelectedValue="{Binding AddingSection.DatesToSelectedItem.Value, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"
                                  SelectedIndex="{Binding AddingSection.DatesToSelectedItem.Index, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"
                                  DropDownClosed="ComboBoxDayTo_DropDownClosed" 
                                  IsEnabled="False"/>
                        <Button x:Name="ButtonTargetDay" Content="{Binding AddingSection.TargetDay, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" Style="{DynamicResource MaterialDesignFlatSecondaryDarkButton}" Padding="4" Foreground="Gray" IsEnabled="False"/>
                    </StackPanel>
                </StackPanel>
                <StackPanel Grid.Row="7" Margin="10,10,10,10">
                    <TextBlock Text="Copy To Every..."/>
                    <StackPanel Orientation="Horizontal" >
                        <ComboBox x:Name="ComboBoxCopy1" Width="85" Margin="0,0,10,0" ItemsSource="{Binding AddingSection.CopyDays1}" DropDownClosed="ComboBoxCopy1_DropDownClosed" IsEnabled="False"/>
                        <ComboBox x:Name="ComboBoxCopy2" Width="85" Margin="0,0,10,0" ItemsSource="{Binding AddingSection.CopyDays2}" Visibility="Hidden" DropDownClosed="ComboBoxCopy2_DropDownClosed" IsEnabled="False"/>
                        <ComboBox x:Name="ComboBoxCopy3" Width="85" Margin="0,0,10,0" ItemsSource="{Binding AddingSection.CopyDays3}" Visibility="Hidden" DropDownClosed="ComboBoxCopy3_DropDownClosed" IsEnabled="False"/>
                    </StackPanel>
                </StackPanel>
                <StackPanel Grid.Row="8">
                    <StackPanel  Margin="10,10,10,10" Orientation="Horizontal" HorizontalAlignment="Center">
                        <Button x:Name="ButtonClear" Content="Clear" Margin="0,0,5,0" Style="{DynamicResource MaterialDesignPaperButton}" Click="ButtonClear_Click" IsEnabled="false"/>
                        <Button x:Name="ButtonAdd" Content="Add" Margin="5,0,0,0" Style="{DynamicResource MaterialDesignPaperButton}" Click="ButtonAdd_Click" IsEnabled="false"/>
                    </StackPanel>
                </StackPanel>
             

                <StackPanel Grid.Row="10" Orientation="Horizontal" HorizontalAlignment="Center">
                    <Image Source="View/Icons/github.png" Margin="18,0,0,0"/>
                    <TextBlock Margin="4,0,10,0" >
                    <Hyperlink NavigateUri="https://github.com/nazar-photo/Schedule" RequestNavigate="Hyperlink_RequestNavigate">
                                    nazar-photo © 2022
                            </Hyperlink>
                    </TextBlock>
                </StackPanel>
            </Grid>
        </Border>
        
    </Grid>

</Window>
