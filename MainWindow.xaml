                    <ComboBox x:Name="ComboBoxMonthTo" Width="80" Margin="0,0,10,0" ItemsSource="{Binding Model.ComboBoxMonths}" DropDownClosed="ComboBoxMonthTo_DropDownClosed"/>
                    <ComboBox x:Name="ComboBoxDayTo" Width="40" Margin="0,0,10,0" ItemsSource="{Binding Model.ComboBoxDates}" DropDownClosed="ComboBoxDayTo_DropDownClosed"/>
                    <Button x:Name="ButtonTargetDay" Content="" Style="{DynamicResource MaterialDesignFlatSecondaryDarkButton}" Padding="4" Foreground="Gray" IsEnabled="False"/>
                </StackPanel>
            </StackPanel>
